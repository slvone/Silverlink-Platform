{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "no-console:": 0,
    "no-alert:": 0,
    "jsx-a11y/label-has-for": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "no-unused-expressions": 0,
    "global-require": 0,
    "no-useless-escape": 0,
    "max-len": 0,
    "react/no-unescaped-entities": 0,
  }
}
